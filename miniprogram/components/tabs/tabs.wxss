.tabs {
  width: 100%;
  height: 100%;
}
.tabs_title{
  display: flex;
  justify-content: space-around;
  height: 60rpx;
  line-height: 58rpx;
  font-size: 30rpx;
}
.title_item{
  text-align: center;
  width: 30%;

}
.active {
  color: var(--themeColor);
  border-bottom: 2rpx solid currentColor;
}
.tabs_content{
  height: calc(100% - 80rpx);
  
  margin-top: 20rpx;
}